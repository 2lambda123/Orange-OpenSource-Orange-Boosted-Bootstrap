{
  "name": "boosted",
  "description": "Orange Boosted is a framework library accessible, ergonomic and Orange branded based on Bootstrap v4.0.0-alpha.2.",
  "version": "4.0.0-alpha.2",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://boosted.orange.com",
  "author": "Twitter, Inc.",
  "scripts": {
    "change-version": "node grunt/change-version.js",
    "shrinkwrap": "npm shrinkwrap --dev && mv ./npm-shrinkwrap.json ./grunt/npm-shrinkwrap.json",
    "test": "grunt test"
  },
  "style": "dist/css/boosted.css",
  "sass": "scss/boosted.scss",
  "main": "./dist/js/npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/issues"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "1.9.1 - 2",
    "tether": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-eslint": "^4.1.3",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.3",
    "grunt-build-control": "^0.6.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.1.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-connect": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.0",
    "grunt-contrib-qunit": "^1.0.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^17.1.0",
    "grunt-exec": "^0.4.6",
    "grunt-html": "^7.0.0",
    "grunt-jekyll": "^0.4.2",
    "grunt-jscs": "^2.8.0",
    "grunt-line-remover": "0.0.2",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^1.2.0",
    "grunt-saucelabs": "^8.6.1",
    "grunt-scss-lint": "^0.3.8",
    "grunt-stamp": "^0.1.0",
    "is-travis": "^1.0.0",
    "load-grunt-tasks": "^3.4.0",
    "mq4-hover-shim": "^0.3.0",
    "shelljs": "^0.6.0",
    "time-grunt": "^1.2.1",
    "serve-static": "^1.10.0",
    "grunt-rtlcss": "^2.0.1",
    "grunt-text-replace": "^0.4.0"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "dist",
    "grunt",
    "js/*.js",
    "scss/**/*.scss",
    "Gruntfile.js",
    "LICENSE"
  ],
  "jspm": {
    "main": "js/bootstrap",
    "ignore": [
      "dist/js/npm"
    ],
    "directories": {
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "deps": [
          "jquery"
        ],
        "exports": "$"
      }
    },
    "dependencies": {
      "jquery": "*"
    }
  }
}
