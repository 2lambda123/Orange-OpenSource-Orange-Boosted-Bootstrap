<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>boosted</id>
    <version>3.4.1</version>
    <title>Boosted CSS</title>
    <authors>The Boosted Authors, Orange, The Bootstrap Authors, Twitter Inc.</authors>
    <owners>Orange</owners>
    <description>Fork of the most popular front-end framework for developing responsive, mobile first projects on the web.</description>
    <releaseNotes>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/releases</releaseNotes>
    <summary>Boosted framework in CSS. Includes fonts and JavaScript</summary>
    <language>en-us</language>
    <projectUrl>https://boosted.orange.com</projectUrl>
    <iconUrl>https://boosted.orange.com/assets/img/favicons/apple-touch-icon.png</iconUrl>
    <licenseUrl>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/blob/master/LICENSE</licenseUrl>
    <copyright>Copyright 2017-2019</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="jQuery" version="[1.9.1,4)" />
    </dependencies>
    <tags>css js less mobile-first responsive front-end framework web</tags>
  </metadata>
  <files>
    <file src="less\**\*.less" target="content\Content\boosted" />
    <file src="fonts\*.*" target="content\Content\fonts" />
    <file src="dist\js\boosted*.js" target="content\Scripts" />
    <file src="less\**\*.less" target="contentFiles\Content\boosted" />
    <file src="fonts\*.*" target="contentFiles\Content\fonts" />
    <file src="dist\js\boosted*.js" target="contentFiles\Scripts" />
  </files>
</package>